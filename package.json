		"jsdom": "^28.1.0",
		"svelte": "^5.51.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"node-html-parser": "^7.0.2"
